{
    "name": "vulcan/rivescript",
    "description": "RiveScript interpreter library for PHP with Laravel support. RiveScript is a scripting language for chatterbots, making it easy to write trigger/response pairs for building up a bot's intelligence.",
    "keywords": ["rivescript", "bot", "ai", "chatterbot", "vulcan"],
    "license": "MIT",
    "authors": [
        {
            "name": "Shea Lewis",
            "email": "shea.lewis89@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "symfony/console": "~3.0",
        "vulcan/verbal-expressions-php": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.0",
        "monolog/monolog": "~1.0"
    },
    "autoload": {
        "files": [
            "src/Support/helpers.php"
        ],
        "psr-4": {
            "Vulcan\\Rivescript\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "minimum-stability": "stable"
}
