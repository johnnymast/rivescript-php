{
  "name": "axiom/rivescript",
  "description": "RiveScript interpreter library for PHP with Laravel support. RiveScript is a scripting language for chatterbots, making it easy to write trigger/response pairs for building up a bot's intelligence.",
  "minimum-stability": "stable",
  "license": "MIT",
  "keywords": [
    "rivescript",
    "bot",
    "ai",
    "chatterbot",
    "axiom"
  ],
  "authors": [
    {
      "name": "Shea Lewis",
      "email": "shea.lewis89@gmail.com"
    },
    {
      "name": "Johnny Mast",
      "email": "mastjohnny@gmail.com"
    }
  ],
  "scripts": {
    "test": [
      "vendor/bin/pest"
    ]
  },
  "autoload": {
    "files": [
      "src\\helpers.php"
    ],
    "psr-4": {
      "Axiom\\Rivescript\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "require": {
    "php": ">=7.4",
    "symfony/console": "^5.0",
    "symfony/finder": "^3.2",
    "axiom/collections": "~2.0"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "*",
    "phpunit/phpunit": "^9.5.5",
    "monolog/monolog": "~1.0",
    "pestphp/pest": "^1.14",
    "phpstan/phpstan": "^0.12.94"
  },
  "extra": {
    "branch-alias": {
      "dev-develop": "1.0.x-dev"
    }
  }
}
