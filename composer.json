{
  "name": "axiom/rivescript",
  "description": "RiveScript interpreter library for PHP with Laravel support. RiveScript is a scripting language for chatterbots, making it easy to write trigger/response pairs for building up a bot's intelligence.",
  "minimum-stability": "stable",
  "license": "MIT",
  "keywords": [
    "rivescript",
    "bot",
    "ai",
    "chatterbot",
    "axiom"
  ],
  "authors": [
    {
      "name": "Shea Lewis",
      "email": "shea.lewis89@gmail.com"
    },
    {
      "name": "Johnny Mast",
      "email": "mastjohnny@gmail.com"
    }
  ],
  "scripts": {
    "test": [
      "vendor/bin/pest"
    ]
  },
  "autoload": {
    "psr-4": {
      "Axiom\\Rivescript\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/",
      "Tests\\Rsts\\": "tests/Rsts/src"
    }
  },
  "require": {
    "php": ">=8.1"
  },
  "require-dev": {
    "pestphp/pest": "^2.5",
    "mockery/mockery": "^1.5"
  },
  "config": {
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  }
}
